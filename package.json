{
  "name": "dependency-visualizer",
  "version": "1.0.0",
  "type": "module",
  "main": "dist-electron/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite --config vite.config.ts",
    "dev:electron": "tsc -p electron.tsconfig.json && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main/main.js",
    "build": "vite build",
    "build:electron": "tsc -p electron.tsconfig.json && electron-builder",
    "dist": "npm run build && npm run build:electron"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.0",
    "electron-builder": "^24.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0",
    "vite": "^5.0.0",
    "vue-tsc": "^2.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "echarts": "^6.0.0",
    "semver": "^7.7.3",
    "vis-network": "^10.0.2",
    "vue": "^3.4.0"
  }
}
